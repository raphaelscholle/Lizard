 Makefile.config             |    4 +-
 include/EGL/egl.h           |    2 +-
 include/EGL/eglext.h        |  541 +++++++++++-
 include/EGL/eglplatform.h   |  132 +--
 include/GLES/gl.h           |   22 +-
 include/GLES/glext.h        |  677 ++++++++++++++-
 include/GLES/glplatform.h   |   11 +-
